									<a href="profile?profileid=${current.id}"> <c:out
											value="${current.name}" />
									</a> <br />
									<c:out value="${current.category}" />
									<br />
									<c:out value="${current.specialization}" />
									<br />

									<c:out value="${current.state}" />
